class Solution:
    def soupServings(self, n: int) -> float:
        n=(n+24)//25

        
        @cache
        def prob(A,B):
            if A<=0 and B<=0:
                return 0.5
            if A<=0:
                return 1.0
            if B<=0:
                return 0.0
            return (prob(A-4,B)+prob(A-3,B-1)+prob(A-2,B-2)+prob(A-1,B-3))/4

        if n>=200:
            return 1.0

        return prob(n,n)
